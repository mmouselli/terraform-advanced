resource "openstack_compute_keypair_v2" "example_keypair" {
  name       = "example_keypair"
  public_key = "ssh-rsa "
}

resource "openstack_compute_secgroup_v2" "example_sg" {
  name        = "example_sg"
  description = "Allow inboud SSH"

  rule {
    from_port   = 22
    to_port     = 22
    ip_protocol = "tcp"
    cidr        = "0.0.0.0/0"
  }
}
